journal = {Psychiatry Research - Neuroimaging},
keywords = {Aging,Anatomy,Cytoarchitecture,MRI},
number = {1},
pages = {67--72},
publisher = {Elsevier Ireland Ltd},
title = {{Volumetry of the human amygdala - An anatomical study}},

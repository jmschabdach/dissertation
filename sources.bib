isbn = {1936-959X (Electronic)$\backslash$r0195-6108 (Linking)},
issn = {01956108},
journal = {American Journal of Neuroradiology},
mendeley-groups = {*Research - CHP PIRC/Use of Prealignment in Global Volume Registration,*Dissertation},

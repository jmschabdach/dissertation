eprint = {NIHMS150003},
file = {:home/jenna/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/van Dijk, Sabuncu, Buckner - 2012 - The influence of head motion on intrinsic functional connectivity MRI.pdf:pdf},
isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {BOLD,Connectome,Functional MRI,Movement,Resting-state},
mendeley-groups = {*Research - CHP PIRC,*Research - CHP PIRC/Use of Prealignment in Global Volume Registration,*Dissertation},

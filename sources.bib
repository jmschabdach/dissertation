issn = {03877604},
journal = {Brain and Development},
keywords = {Amplitude,Attention deficit hyperactivity disorder,Low-frequency fluctuation,Resting-state functional MRI,Spontaneous neuronal activity},
mendeley-groups = {*Research - CHP PIRC/Use of Prealignment in Global Volume Registration,*Dissertation},

pages = {267--277},
pmid = {25770991},
publisher = {Elsevier Inc.},
title = {{ICA-AROMA: A robust ICA-based strategy for removing motion artifacts from fMRI data}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2015.02.064},

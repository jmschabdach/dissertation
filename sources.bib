issn = {0730725X},
journal = {Magnetic Resonance Imaging},
keywords = {Bloch equations,MRI simulations,Neuronal current imaging,Transient magnetic fields},
mendeley-groups = {*Research - CHP PIRC/Use of Prealignment in Global Volume Registration,*Dissertation},
number = {7},
pages = {1014--1021},
